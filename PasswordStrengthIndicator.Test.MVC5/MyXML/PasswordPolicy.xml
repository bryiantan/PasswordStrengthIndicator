<?xml version="1.0" encoding="utf-8" ?>
<?xml-stylesheet type="text/xsl" href="PasswordPolicy.xslt"?>
<PasswordPolicy>
  <Password>
    <duration>180</duration>
    <minLength>12</minLength>
    <maxLength>25</maxLength>
    <numsLength>2</numsLength>
    <upperLength>1</upperLength>
    <specialLength>1</specialLength>
    <barWidth>200</barWidth>
    <barColor>Green</barColor>
    <specialChars>!@#\\$%*()_+^&amp;}{:;?.</specialChars>
    <!-- 1=Yes, 0=No-->
    <useMultipleColors>1</useMultipleColors>
    <!-- 0=allow repeat, 1 = allow 1 maximum number of successive repetitions of a given character-->
    <maxConsecutiveRepeatedChars>1</maxConsecutiveRepeatedChars>
<maxKeyboardSequence>3</maxKeyboardSequence>
    <keyboardSequenceCharacters>zaq12wsxcde34rfvbgt56yhnmju78ik,.lo90p;/poiuytrewqlkjhgfdsamnbvcxz~!@#$%^&amp;*()_+`1234567890-==-0987654321`qwertyuiopasdfghjklzxcvbnm1qazxsw23edcvfr45tgbnhy67ujm,ki89ol./;p0abcdefghijklmnopqrstuvwxyzzyxwvutsrqponmlkjihgfedcba!qazxsw23edcvfr45tgbnhy67ujm,ki89ol./;p0poiuytrewqlkjhgfdsamnbvcxz</keyboardSequenceCharacters>

  </Password>
</PasswordPolicy>